<odoo>
    <record id="paperformat_patient" model="report.paperformat">
        <field name="name">Custom Paper</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">297</field>
        <field name="page_width">210</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">50.00</field>
        <field name="margin_bottom">20.00</field>
        <field name="margin_left">2.0</field>
        <field name="margin_right">7.0</field>
        <field name="dpi">100</field>
    </record>
    <record id="paperformat_patient_appointment" model="report.paperformat">
        <field name="name">Custom Paper</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">297</field>
        <field name="page_width">210</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">5.00</field>
        <field name="margin_bottom">20.00</field>
        <field name="margin_left">2.0</field>
        <field name="margin_right">7.0</field>
        <field name="dpi">100</field>
    </record>
</odoo> 