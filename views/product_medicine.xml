<odoo>
    <data>
        <record id="product_list_view" model="ir.ui.view">
            <field name="name">product.list.view</field>
            <field name="model">product.description</field>
            <field name="arch" type="xml">
                <list>
                    <field name="invoice" />
                    <field name="description" />
                    <field name="product" />
                    <field name="quantity" />
                    <field name="start" />
                    <field name="end" />
                </list>
            </field>
        </record>
    </data>
</odoo>